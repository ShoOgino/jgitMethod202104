	private File[] pack(Repository src, RevObject... list)
			throws IOException {
		try (PackWriter pw = new PackWriter(src)) {
			for (RevObject o : list) {
				pw.addObject(o);
			}

			final ObjectId name = pw.computeName();
			final PackFile packFile = fullPackFileName(name, ".pack");
			final PackFile idxFile = packFile.create(PackExt.INDEX);
			final File[] files = new File[] { packFile, idxFile };
			write(files, pw);
			return files;
		}
	}

