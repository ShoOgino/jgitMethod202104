	private static boolean isTokenChar(int ch) {
		switch (ch) {
		case '-':
		case '.':
		case '/':
		case '_':
		case ':':
		case '*':
		case '+':
		case '=':
			return true;
		default:
			if ((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z')
					|| (ch >= '0' && ch <= '9')) {
				return true;
			}
			return false;
		}
	}

