	/** {@inheritDoc} */
	@Override
	protected void show(final ObjectWalk ow, final RevObject obj)
			throws Exception {
		if (obj.has(RevFlag.UNINTERESTING))
			outw.print('-');
		obj.getId().copyTo(outbuffer, outw);
		final String path = ow.getPathString();
		if (path != null) {
			outw.print(' ');
			outw.print(path);
		} else if (obj instanceof RevTree)
			outw.print(' ');
		outw.println();

	}

