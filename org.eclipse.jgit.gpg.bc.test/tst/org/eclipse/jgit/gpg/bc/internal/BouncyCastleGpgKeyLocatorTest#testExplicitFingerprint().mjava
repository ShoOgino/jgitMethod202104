	@Test
	public void testExplicitFingerprint() throws Exception {
		assertFalse(match("John Fade <j.fade@example.com>", "0xfade"));
		assertFalse(match("John Fade <0xfade@example.com>", "0xfade"));
		assertFalse(match("John Fade <0xfade@example.com>", "0xFADE"));
		assertFalse(match("", "0xfade"));
	}

